<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <connectionStrings>
        <add name="DddExample"
             providerName="System.Data.SqlClient"
             connectionString="Server=.;Database=DddExample;Trusted_Connection=True;" />
    </connectionStrings>
    <appSettings>
        <add key="NHibernate.ShowSql"
             value="true" />
    </appSettings>
    <startup>
        <supportedRuntime version="v4.0"
                          sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <runtime>
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
            <dependentAssembly>
                <assemblyIdentity name="NHibernate"
                                  publicKeyToken="aa95f207798dfdb4"
                                  culture="neutral" />
                <bindingRedirect oldVersion="0.0.0.0-5.1.0.0"
                                 newVersion="5.1.0.0" />
            </dependentAssembly>
        </assemblyBinding>
    </runtime>
</configuration>
